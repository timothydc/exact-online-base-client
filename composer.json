{
  "name": "timothydc/exact-online-base-client",
  "description": "Exact Online base client",
  "license": "MIT",
  "keywords": [
    "Exact Online"
  ],
  "authors": [
    {
      "name": "Timothy De Cort"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "monolog/monolog": "^1.26|^2.2",
    "picqer/exact-php-client": "^4.2",
    "symfony/filesystem": "^4.4|^5.4|^6.3",
    "symfony/lock": "^4.4|^5.4|^6.3"
  },
  "autoload": {
    "psr-4": {
      "TimothyDC\\ExactOnline\\BaseClient\\": "src"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "require-dev": {
    "symplify/easy-coding-standard": "11.1.12"
  }
}
